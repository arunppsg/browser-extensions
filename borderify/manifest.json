{
    "manifest_version": 2,
    "name": "Borderify",
    "version": "1.0",

    "description": "Adds a red border to google domains",

    "icons": {
        "48": "icons/border-48.png"
    },

    "content_scripts": [
        {
            "matches": ["*://*.google.com/*"],
            "js": ["borderify.js"]
        }
    ]
}
